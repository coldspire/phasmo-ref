---
layout: baseLayout
---
<h2>Tarot cards</h2>
{%- for card in tarot %}
<ul>
    <li>{{ card.name }}</li>
    <li>{{ card.effect }}</li>
    <li>{{ card.burnColor }}</li>
    <li>{{ card.drawChance }}</li>
    {% if card.notes.length > 0 %}
    <li> Also note:
        <ul>
            {% for note in card.notes %}
                <li>{{ note }}</li>
            {% endfor %}
        </ul>
    </li>
    {% endif %}
</ul>
{% endfor -%}